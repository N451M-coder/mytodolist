{
    "name": "LifeList",
    "description": "Todo List with motivational quotes, it will show on every new tab",
    "version": "0.0.1",
    "manifest_version": 3,
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/todo.png"
          }
      },
      "icons": {
        "128": "/images/todo.png"
      },
      "permissions": [
        "tabs"
      ],
      
    "author": "Nasim"
  }
  
