{
    "name": "My Todos",
    "description": "Todo List with motivatinal quotes, it will show on every new tab",
    "version": "0.0.1",
    "manifest_version": 3,
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/todo.png"
          }
      },
      "icons": {
        "128": "/images/todo.png"
      },
      "chrome_url_overrides" : {
        "newtab": "todos.html"
      },
      "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'",
    "author": "Nasim"
  }
  
